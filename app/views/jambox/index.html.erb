<h1>Jukebox Jams</h1>

<div class="chance-block">
<%= button_to("Get Random Jamz", chance_path, method: :get, class: "chance") %>
</div>

<div class="form-block">
<%= form_tag(root_path, method: :get) do %>
  <%= label_tag("Search for artist") %>
  <%= text_field_tag(:artist) %>
  <%= submit_tag("Search", class: "button") %>
</div>
<div class="media">
  <iframe id="iframe" width="300" height="200"></iframe>
</div>
<% end %>
